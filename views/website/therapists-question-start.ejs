#msform2 .form-card .option-block {
    margin-top: 20px;
    background: var(--color-white);
    box-shadow: rgba(99, 99, 99, 0.2) 0px 2px 8px 0px;
    margin: 20px 0px;
    padding: 30px;
}
.option-block .radio-input {
  display: inline-block;
  flex-direction: row;
  font-size: 16px;
  font-weight: 600;
  width: 50%;
  
}
.form-card.Therapys-option .radio-input {
    display: grid;
    grid-template-columns: repeat(2,1fr);
    margin: 0 auto;
    gap: 30px;
    width: 100%;
}
.option-block .radio-input input[type="radio"] {
  display: none;
}

.option-block .radio-input label {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 20px 10px;
    border: 1px solid #ccc;
    background-color: rgb(70 188 150 / 34%);
    border-radius: 100px;
    cursor: pointer;
    position: relative;
    transition: all 0.3s ease-in-out;
    margin-bottom: 30px;
    color: var(--color-black);
    font-family: 'gilroy-sb';
    text-transform: capitalize;
}

.option-block .radio-input label:before {
  content: "";
  display: block;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate(-50%, -50%);
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: #fff;
  border: 2px solid #ccc;
  transition: all 0.3s ease-in-out;
  opacity: 0;
}

.option-block .radio-input input[type="radio"]:checked + label:before {
  background-color: #46bc96;
  top: 0;
  opacity: 0;
}

.option-block .radio-input input[type="radio"]:checked + label {
  background-color: var(--color-green);
    color: #fff;
    border-color: var(--color-green);
    animation: radio-translate 0.5s ease-in-out;
}

.form-card.Therapys-option .radio-input label {
    flex: 1 0 45%;
    margin-bottom:0;
}
@keyframes radio-translate {
  0% {
    transform: translateX(0);
  }

  50% {
    transform: translateY(-10px);
  }

  100% {
    transform: translateX(0);
  }
}
.select-menu {
    width: 80%;
    margin: 0 auto;
    cursor: pointer;
    position: relative;
}

.select-menu > .select {
    border: 1px solid #ccc;
